{
  "name": "@jitesoft/webpage",
  "version": "2.0.0",
  "description": "Jitesoft Webpage.",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.4.2",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@jitesoft/babel-preset-main": "^1.1.3",
    "@jitesoft/on-load": "gist:5a23bb38e2f802b43806381d3472032f",
    "@vue/babel-preset-app": "^3.5.2",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "core-js": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "semistandard": "^13.0.1",
    "webpack-dev-server": "^3.2.1"
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "watch": "cross-env NODE_ENV=development webpack-dev-server",
    "dev": "npm run watch",
    "test": "semistandard /src"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:vue/recommended",
      "semistandard"
    ],
    "overrides": [
      {
        "files": [
          "webpack/*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  },
  "meta": {
    "title": "Jitesoft",
    "template": "src/template.html",
    "og": {
      "title": "Jitesoft",
      "description": "A company specialized in backend development for Web, Games and Applications.",
      "image": "img/Jitesoft-scaled.png",
      "url": "https://jitesoft.com"
    },
    "twitter": {
      "card": "summary_large_image",
      "image": "img/Jitesoft-scaled.png",
      "description": "A company specialized in backend development for Web, Games and Applications.",
      "title": "Jitesoft"
    },
    "description": "A company specialized in backend development for Web, Games and Applications."
  }
}
