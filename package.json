{
  "name": "@jitesoft/webpage",
  "version": "2.0.0",
  "description": "Jitesoft Webpage.",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@jitesoft/babel-preset-main": "^2.1.3",
    "@jitesoft/on-load": "gist:5a23bb38e2f802b43806381d3472032f",
    "@jitesoft/yolog": "^2.7.2",
    "@vue/babel-preset-app": "^4.2.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "url-loader": "^3.0.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-router": "^3.1.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "@jitesoft/eslint-config": "^1.10.0",
    "webpack-dev-server": "^3.10.3"
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack -p",
    "watch": "cross-env NODE_ENV=development webpack-dev-server",
    "dev": "npm run watch",
    "test": "eslint --ext .js,.vue src/"
  },
  "eslintConfig": {
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "@jitesoft"
    ],
    "overrides": [
      {
        "files": [
          "webpack/*.js"
        ],
        "env": {
          "node": true
        }
      }
    ]
  },
  "meta": {
    "title": "Jitesoft",
    "template": "src/template.html",
    "og": {
      "title": "Jitesoft",
      "description": "A company specialized in backend development & ops for Web, Games and Applications.",
      "image": "img/og-image.png",
      "url": "https://jitesoft.com"
    },
    "twitter": {
      "card": "summary_large_image",
      "image": "img/og-image.png",
      "description": "A company specialized in backend development & ops for Web, Games and Applications.",
      "title": "Jitesoft"
    },
    "description": "A company specialized in backend development & ops for Web, Games and Applications."
  }
}
